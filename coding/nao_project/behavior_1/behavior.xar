                  <![CDATA[import socket

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self):
        self.initServer()
        pass

    def initServer(self):
        host = '192.168.0.151' #school robot ip
        port = 5050

        self.s = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
        self.s.connect((host, port))

        self.logger.info('Client connected to IP: ' + host)

        self.runServer()

    def runServer(self):
        while True:
            message = self.s.recv(1024)
            if message != None and message != '':
                self.logger.info(str('Recieved from app: ' + message))

                message = message.split('_')

                if 'nao' in message[2] and message[0] == 'arduino':
                    self.onReceivedMessage(str(message[1]))
                elif 'nao' in message[2] and message[0] == 'app' and 'quiz' in message[1]:
                    self.onReceivedQuestion(str(message[1]))
                else:
                    message = ['', '', '']
                #return message
        self.s.close()


    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box

    def onInput_sendMessage(self, msg):
        self.s.send(msg.encode())]]>
