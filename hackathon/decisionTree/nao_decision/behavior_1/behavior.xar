    def onLoad(self):
        #put initialization code here
        pass

    def onUnload(self):
        #put clean-up code here
        pass

    def onInput_onStart(self, path):
        try:
            url = 'http://' + self.getParameter("Server IP") + ':5000/api/' + path
            response = requests.post(url)

            if response.status_code == 200:
                self.logger.info("File inviato")

                self.onStopped(str(response.json()['result']))
            else:
                self.logger.error("Errore invio file: " + str(response.json()))

        except Exception as e:
            self.logger.error(e)

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped('') #activate the output of the box]]>
